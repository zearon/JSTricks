{
	"background": {
		"page": "background.html"
	},
	"browser_action": {
		"default_icon": "icon.png",
		"default_popup": "popup.html"
	},
	"content_scripts": [ {
		"js": [ "js/sea-debug.js", "js/seajs_boot.js", "autoload.js"],
		"css": ["css/custom-theme/jquery-ui.css", "css/custom-theme/jquery-ui-fix.css"],
		"matches": [ "http://*/*", "https://*/*", "file://*/*" ],
		"run_at": "document_start"
	} ],
	"web_accessible_resources": [
		"js/jquery.js", "js/jquery-ui.js", "js/dialogextend.js", "js/msgbox.js", "js/selectionBox.js",
		"injected.js", "popup.js", "popup.html", "options.js", "options.html"],
	"description": "Add your own JavaScript (powered by JQuery) and/or CSS to any page to trick it!",
	"icons": {
		"128": "icon128.png",
		"16": "icon16.png",
		"24": "icon24.png",
		"48": "icon48.png"
	},
	"manifest_version": 2,
	"name": "JavaScript Tricks",
	"options_page": "options.html",
	"permissions": [ "contextMenus", "activeTab", "tabs", "cookies", "http://*/*", "https://*/*"],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.0.2"
}
