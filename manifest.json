{
	"background": {
		"persistent": false,
		"page": "background.html"
	},
	"page_action": {
		"default_icon": {
			"19":"icon/ICON19.png",
			"38":"icon/ICON38.png"
		},
		"default_popup": "popup.html"
	},
	"content_scripts": [ {
		"js": [
			"injected/plugin/care-your-eyes.js",
			"injected/dom.js", "injected/sea-debug.js", "injected/seajs_boot.js", "injected/ready.js", 
			"injected/autoload.js", "injected/plugin/loader.js", "injected/nodeSelector.js"],
		"css": ["css/jquery-ui.structure.css", "css/theme/jqueryui/jquery-ui.theme-light.css"],
		"matches": [ "http://*/*", "https://*/*", "file://*/*" ],
		"run_at": "document_start"
	} ],
	"web_accessible_resources": ["lib/jquery.js", "lib/jquery-ui.js", 
		"injected/ready.js", "injected/msgbox.js", "injected/nodeSelector.js", "injected/selectionBox.js", 
		"injected/injected.js", "popup.html"],
	"sandbox": ["sandbox.html"],
	"description": "Add your own JavaScript (powered by JQuery) and/or CSS to any page to trick it!",
	"icons": {
		"128": "icon/icon128.png",
		"16": "icon/icon16.png",
		"24": "icon/icon24.png",
		"48": "icon/icon48.png"
	},
	"manifest_version": 2,
	"name": "JavaScript Tricks",
	"options_page": "options.html",
	"permissions": [ "declarativeContent", "webNavigation", "contextMenus", "activeTab", 
		"tabs", "cookies", "storage", "unlimitedStorage", "http://*/*", "https://*/*"],
	"update_url": "https://clients2.google.com/service/update2/crx",
	"version": "1.1.1.002"
}
